{
  "name": "codecov-action",
  "version": "1.2.0",
  "description": "Upload coverage reports to Codecov from GitHub Actions",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test-calculator": "jest --testPathPattern=demo/calculator/ --coverage --coverageDirectory=coverage/calculator",
    "test-coverage": "jest --testPathPattern=demo/coverage --coverage --coverageDirectory=coverage/coverage",
    "test-script": "jest --testPathPattern=src --coverage --coverageDirectory=coverage/script",
    "test-all": "yarn run test && yarn run test-calculator && yarn run test-index && eslint",
    "build": "ncc build src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codecov/codecov-action.git"
  },
  "keywords": [],
  "author": "Ibrahim Ali",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codecov/codecov-action/issues"
  },
  "homepage": "https://github.com/codecov/codecov-action#readme",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@types/jest": "^26.0.19",
    "@zeit/ncc": "^0.22.3",
    "fs": "0.0.1-security",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "request": "^2.88.2",
    "requestretry": "^4.1.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "eslint": "^7.17.0"
  }
}
